<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Geral.TelasFront.TelaCadasPJ1">


        <TextView
            android:id="@+id/TextInicio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Torne-se nosso parceiro!,"
            android:layout_marginTop="100dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/BotaoCadas"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Cadastro"
            android:layout_marginTop="100dp"
            app:layout_constraintTop_toTopOf="@+id/BotaoLoginIni"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

        <Button
            android:id="@+id/BotaoLoginIni"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="Login"
            android:layout_marginTop="100dp"
            app:layout_constraintTop_toTopOf="@+id/TextInicio"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

</androidx.constraintlayout.widget.ConstraintLayout>